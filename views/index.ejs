<!DOCTYPE html>
<html>
    <%- include partial/head %>
    <link href="/stylesheets/dropdown.css" rel="stylesheet" type="text/css">
    <script src="/javascripts/dropdown.js"></script>
  <body>
    <div>
      <form class="" action="/order/add/<%=amount%>" method="POST">
      <%for(var i = 0; i < amount; i++) {%>
        <table>
        <tr>Order</tr>
        <tr><input type="text" name="order" value="" placeholder="First Name"></tr>
        <tr>Price</tr>
        <tr><input type="number" name="price" value="" placeholder="Last Name"></tr>
        <tr>Payee</tr>
            <tr>
            <select name=<%=i%> multiple>
              <option value="satu">Satu</option>
              <option value="dua">Dua</option>
              <option value="tiga">Tiga</option>
              <option value="empat">Empat</option>
              <option value="lima">lima</option>
              <option value="enam">enam</option>
              <option value="tujuh">tujuh</option>
              <option value="delapan">delapan</option>
            </select>
          </tr>
          </div>
        <%}%>
      </table>
        <input type="submit" value="Save">
      </form>
      <a href="/addPayee">Add a new payee</a>
  </body>
</html>
