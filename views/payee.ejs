<!DOCTYPE html>
<html>
  <%- include partial/head %>
  <%- include partial/login %>
  <body>
    <body>
      <div class="col-md-10 col-md-offset-1">
        <div class="wrapper">
         <form class="form-signin" action="/login" method="post">
          <h2>Add Payee</h2><br>
           <input type="text" class="form-control" name="name" placeholder="Payee Name"/><br>
           <input type="text" class="form-control" name="email" placeholder="Email"/><br>
           <button class="btn btn-lg btn-primary btn-block" type="submit">Add Payee</button><br>
            <select class="" name="#">
                <% .forEach(row => { %>
                    <option value="<%=row.id%>"><%=row.subject_name%></option>
                <% }) %>
            </select><br><br>
            <input type="submit" value="Add Subject">
         </form>
        </div>
      </div>
  </body>
</html>